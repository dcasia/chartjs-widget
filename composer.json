{
    "name": "digital-creative/chartjs-widget",
    "description": "A chartjs widget for laravel nova-dashboard.",
    "keywords": [
        "laravel",
        "nova",
        "chartjs",
        "dashboard",
        "nova-dashboard"
    ],
    "authors": [
        {
            "name": "Rafael Milewski"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ozdemirburak/iris": "^2.1",
        "timothyasp/nova-color-field": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\ChartJsWidget\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\ChartJsWidget\\WidgetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
